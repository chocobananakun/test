{
  "name": "unity-webgl-singlefile",
  "version": "1.0.0",
  "description": "Bundle Unity WebGL build into a single HTML file",
  "scripts": {
    "build-single": "node build-to-singlefile.js ./Build ./index-singlefile.html",
    "build-single-cwd": "node build-to-singlefile.js . ./index-singlefile.html",
    "serve-http-server": "npx http-server -c-1 -p 8000",
    "serve-python": "python -m http.server 8000"
  },
  "engines": {
    "node": ">=14"
  },
  "license": "MIT"
}
